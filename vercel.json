{
  "rewrites": [
    {
      "source": "/ai-automation",
      "destination": "/ai-vibe-coders.html"
    },
    {
      "source": "/canvas",
      "destination": "/canvas-with-apis.html"
    },
    {
      "source": "/blog",
      "destination": "/blog-viewer.html"
    },
    {
      "source": "/automation",
      "destination": "/ai-vibe-coders.html"
    },
    {
      "source": "/creative",
      "destination": "/canvas-with-apis.html"
    },
    {
      "source": "/research",
      "destination": "/blog-viewer.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options", 
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}